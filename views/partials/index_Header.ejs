<div class="top-header">
    <div class="container hidden-sm hidden-xs">
        <div class="row" style="position: relative;">
            <div class="col-sm-12 col-md-1">
                <a href="/"><img src="img/LOGO.svg" alt="LienHoaCac" style="width: 80px; margin: auto; padding-top: 10px;"></a>
            </div>
            <form class="form-inline col-sm-12 col-md-5" method="GET" action="/search" style="position: absolute; top: 50%; left: 100px; transform: translate(0%, -50%)">
                <div class="form-group">
                    <input type="text" class="form-control" name="keyword" id="topSearchKeyword" placeholder="Tìm kiếm sản phẩm">
                </div>
                <button type="submit" class="btn btn-default">Tìm</button>
            </form>
            <div class="col-md-6 col-md-offset-5" style="position: absolute; top: 50%; left: 100px; transform: translate(0%, -50%)">
                <div class="col-md-4 col-md-offset-5" style="padding: 0px;">
                    <!-- <div class="row"> -->
                        <div class="col-md-4" style="padding: 0px;">
                            <a href="/edit-account?email=<%=data.emailheader%>">
                            <img class="img-circle origin" src="img/40x40.png" alt="Image Alternative text" title="AMaze" />                            
                            </a> 
                        </div>
                        <div class="col-md-8" style="padding: 0px;">
                            <h5>Hi, <%= data.fname %></h5>
                        </div>
                   <!--  </div>  -->           
                </div>
                <div class="col-md-3" style="padding: 0px;">
                    <a href="/logout">
                        <h5>Sign Out</h5>
                    </a>
                </div>
            </div>
        </div>
    </div> 
</div>
