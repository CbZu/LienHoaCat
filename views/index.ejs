<!DOCTYPE html>
<html lang="en">
<% include partials/index_Header.ejs %>
<body>
<!-- PORTFOLIO -->

<div class="portfolio">
    <div class="container">

        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                <div id="myCarousel" class="carousel slide" data-ride="carousel">
                    <!-- Indicators -->
                    <ol class="carousel-indicators">
                        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                        <li data-target="#myCarousel" data-slide-to="1"></li>
                        <li data-target="#myCarousel" data-slide-to="2"></li>
                    </ol>

                    <!-- Wrapper for slides -->
                    <div class="carousel-inner">
                        <div class="item active">
                            <img src="img/backgrounds/phone.jpg" alt="image">
                        </div>
                        <div class="item">
                            <img src="img/backgrounds/beach.jpg" alt="image">
                        </div>
                        <div class="item">
                            <img src="img/backgrounds/road.jpg" alt="image">
                        </div>
                    </div>

                    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
                        <span class="glyphicon"></span>
                    </a>
                    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
                        <span class="glyphicon"></span>
                    </a>
                </div>
            </div>
        </div>


        <!-- categories  -->
        <div class="row">
            <div class="row categories-grid">
                <nav class="categories">
                    <ul class="portfolio_filter">
                        <li class="col-md-1"><a href="" class="active" data-filter="*">Home</a></li>
                        <!-- <% var folder = '';
                        for(i = 0 ; i < result.length ; i++) { %>
                            <% if(result[i].folder_name != folder){ %>
                                <li class="col-md-2"><a href="" data-filter=".folder<%= result[i].folder_id %>"><%= result[i].folder_name %></a></li>
                            <% folder = result[i].folder_name;} %>
                        <% } %>-->
                        <li class="col-md-2"><a href="" data-filter=".folder1">Xem Tuổi và Mệnh</a></li>
                        <li class="col-md-2"><a href="" data-filter=".folder2">Phong Thủy</a></li>
                        <li class="col-md-2"><a href="" data-filter=".folder3">Thờ Cúng</a></li>
                        <li class="col-md-2"><a href="" data-filter=".folder4">Đời Sống</a></li>
                        <li class="col-md-2"><a href="" data-filter=".folder5">Dịch Vụ</a></li>
                    </ul>
                    <% if(type == 1){ %>
                    <ul class="portfolio_filter">
                        <li class="col-md-2"><a href="" data-filter=".admin">Administrator</a></li>
                    </ul>
                    <%}%>
                </nav>
            </div>
        </div>

        <!-- all works -->
        <div class="row">
            <div class="row portfolio_container">

                <% for(var j = 0 ; j < result.length ; j++){ %>

                <div class="col-md-3 folder<%= result[j].folder_id %>">
                    <a href="/maintenance-prd?catflt=<%= result[j].cat_name %>&catId=<%= result[j].cat_id %>"
                       class="portfolio_item work-grid">
                        <img src="assets/img/<%= result[j].image %>" alt="image">
                        <div class="portfolio_item_hover">
                            <div class="item_info">
                                <span><%= result[j].cat_name %></span>
                                <em>photography</em>
                            </div>
                        </div>
                    </a>
                </div>

                <% } %>


                <!-- end single work -->
                <% if(type == 1){ %>
                <!-- Admin -->
                <div class="col-md-3 admin">
                    <a href="maintenance" class="portfolio_item work-grid">
                        <img src="assets/img/accounts.jpg" alt="image">
                        <div class="portfolio_item_hover">
                            <div class="item_info">
                                <span>Tài Khoản</span>
                                <em>photography</em>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="col-md-3 admin">
                    <a href="show-payment" class="portfolio_item work-grid">
                        <img src="assets/img/payment.jpg" alt="image">
                        <div class="portfolio_item_hover">
                            <div class="item_info">
                                <span>Đơn Hàng</span>
                                <em>photography</em>
                            </div>
                        </div>
                    </a>


                </div>
                <div class="col-md-3 admin">
                    <a href="/maintenance-cat" class="portfolio_item work-grid">
                        <img src="assets/img/AddIcon-2 (2).jpg" alt="image">
                        <div class="portfolio_item_hover">
                            <div class="item_info">
                                <span>Thư Mục</span>
                                <em>photography</em>
                            </div>
                        </div>
                    </a>


                </div>
                <% } %>

            </div>
            <!-- end row -->
        </div>
        <!-- all works end -->
        <!-- Modal -->

    </div>
    <!-- end container -->

</div>
<!-- portfolio -->

