    <script>
        function insert(){
            var element = '<div class="row">\n' +
                '                            \n' +
                '                            <h4 class="col-md-5" >\n' +
                '\n' + 'Name' +
                '                            </h4>\n' +
                '                            \n' +
                '\n' +
                '                            <h4 class="col-md-6" >\n' +
                '\n' + 'Image' +
                '                            </h4>\n</div>'
             element += '<div class="row">\n' +
                '                            \n' +
                '                            <input class="col-md-5" id = \'newCat\' value = \'\'>\n' +
                '\n' +
                '                            </input>\n' +
                '                            \n' +
                '\n' +
                '                            <input class="col-md-6" id = \'newImg\' value = \'\'>\n' +
                '\n' +
                '                            </input>\n' +

                '                        </div><br>';
            $(element).insertBefore($('#InsertBtn'));
            document.getElementById('InsertBtn').style.display = 'none';
            document.getElementById('Ok').style.display = 'block';
        }

        function OK(){
            var allElements = document.getElementsByTagName("input");
            var cats = new Array();
            var images = new Array();
            for(var i=0; i<allElements.length; i++) {
                if(allElements[i].id == 'newCat'){
                    cats.push(allElements[i].value);
                }
                if(allElements[i].id == 'newImg'){
                    images.push(allElements[i].value);
                }
            }
            $.ajax({
                type: "POST",
                url: "./PetMart/add-category",
                data: "mode=update&description=" + cats+"&image="+images,
                success: function (msg) {
                    //alert(msg);
                    data = msg;

                    location.reload();

                }
            });
        }
    </script>
