        <div class="row">

            <div class="col-md-12">

                <ul class="booking-list">
                    </br>

                    <div class="row">
                        <div class="col-md-3">
                            <h4 class="booking-item-title">Category</h4>
                        </div>
                        <div class="col-md-3">
                            <h4 class="booking-item-title">Name</h4>
                        </div>
                        <div class="col-md-2">
                            <h4 class="booking-item-title">Size</h4>
                        </div>
                        <div class="col-md-2">
                            <h4 class="booking-item-title">Price</h4>
                        </div>
                        <div class="col-md-2">
                            <h4 class="booking-item-title">Discount</h4>
                        </div>

                    </div>
                    <% for(i = 0;i < result.length;i++){ %>


                    <li>
                        <a class="booking-item" href="/product-detail?id=<%= result[i].product_id%>">
                            <div class="row">

                                <div class="row">
                                    <div class="col-md-3">
                                        <h5 class="booking-item-title"><%= result[i].cat_name %></h5>
                                    </div>
                                    <div class="col-md-3">
                                        <p class="booking-item-description">
                                            <%=result[i].name%>
                                        </p>
                                    </div>
                                    <div class="col-md-6">
                                        <p class="booking-item-description">
                                            <%for (j = 0 ; j < result[i].sizes.split(',').length ; j++){%>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <p class="booking-item-description"> <%= result[i].sizes.split(',')[j]%></p>
                                                </div>
                                                <div class="col-md-4">
                                                    <p class="booking-item-description">
                                                        <%= result[i].prices.split(',')[j]%>
                                                    </p>
                                                </div>
                                                <div class="col-md-4">
                                                    <p class="booking-item-description">
                                                        <%= result[i].disct_prices.split(',')[j] %>
                                                    </p>
                                                </div>
                                            </div>

                                        <%}%>
                                        </p>
                                    </div>




                                </div>

                            </div>
                        </a>
                    </li>
                    <% } %>


