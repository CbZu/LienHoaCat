        <br/>
        <div class="row">
            <div class="row portfolio_container">
                <% for(j = 0;j < result.length;j++){ %>
                <div class="col-md-4">
                    <a href="/product-detail?id=<%= result[j].product_id %>" class="portfolio_item work-grid">
                        <img src="assets/img/<%= result[j].image %>" alt="image">
                        <h3>
                            <% if(result[j].disct_prices == null){ %>
                            <label style=" text-align: right"><font
                                        color="red"><%= result[j].prices.split(';')[0] %></font></label>
                            <% } else { %>
                            <label style="text-decoration:line-through ; text-align: right"><font
                                        color="red"><%= result[j].prices.split(';')[0] %></font></label>
                            ->
                            <label style=" text-align: right"><font
                                        color="red"><%= result[j].disct_prices.split(';')[0] %></font></label>
                            <% } %>

                        </h3>
                        <div class="portfolio_item_hover">
                            <div class="item_info">
                                <span><%= result[j].name %></span>
                            </div>
                        </div>
                    </a>

                </div>
                <% } %>

